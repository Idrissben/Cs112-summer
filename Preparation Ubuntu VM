R

install.packages('Matching')

install.packages('rgenoud')

library(Matching)

library(rgenoud)

#edit link and name
outnonlin_2 = read.csv('https://docs.google.com/spreadsheets/d/e/2PACX-1vTd4G5-TabJLI77bI8p8rcePQXM_OYJ-z2vD8s-cMYYGG2r2EJ_DwKnw01cKjIsqXmD7ABwbcJVhhLl/pub?gid=305126105&single=true&output=csv')

X = cbind(outnonlin_2$X0,outnonlin_2$X1,outnonlin_2$X2)
X = X+1 

Tr = outnonlin_2$T 

#(Here copy paste NewMatch)

NewMatch_outnonlin_2 = NewMatch(X = X, Tr = Tr, pop.size = 40)

