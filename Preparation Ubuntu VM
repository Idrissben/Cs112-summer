install.packages('Matching')

install.packages('rgenoud')

#edit link and name
outnonlin_1 = read.csv('https://docs.google.com/spreadsheets/d/e/2PACX-1vQ0ZLMEnfetqd6DgkZP6iUgG6MArRqQVJJHUxpXEO39m5vR08FTbyj-YLw2wTq1657zI1cSGqqmwY_N/pub?gid=1117723108&single=true&output=csv')

X = cbind(outnonlin_1$X0,outnonlin_1$X1,outnonlin_1$X2)
X = X+1 

Tr = outnonlin_1$T 

NewMatch(X = X, Tr = Tr, pop.size = 40)

